          ]
        },
        {
          "name": "char",
          "type": "tag",
          "attrs": {
            "style": "wj"
          },
          "items": [
            {
              "text": "That whosoever believeth in him should not perish, but have eternal life.",
              "type": "text",
              "attrs": {
                "verseId": "JHN.3.15",
                "verseOrgIds": [
                  "JHN.3.15"
                ]
              }
            }
          ]
        }
      ]
    },
    {
      "name": "para",
      "type": "tag",
      "attrs": {
        "style": "p"
      },
      "items": [
        {
          "name": "verse",
          "type": "tag",
