            },
            {
              "name": "char",
              "type": "tag",
              "attrs": {
                "style": "add"
              },
              "items": [
                {
                  "text": "doth gather",
                  "type": "text",
                  "attrs": {
                    "verseId": "LUK.13.34",
                    "verseOrgIds": [
                      "LUK.13.34"
                    ]
                  }
                }
              ]
            },
            {
              "text": " her brood under ",
              "type": "text",
              "attrs": {
                "verseId": "LUK.13.34",
                "verseOrgIds": [
                  "LUK.13.34"
                ]
