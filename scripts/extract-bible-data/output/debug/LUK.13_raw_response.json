          "text": " ",
          "type": "text",
          "attrs": {
            "verseId": "LUK.13.33",
            "verseOrgIds": [
              "LUK.13.33"
            ]
          }
        },
        {
          "name": "verse",
          "type": "tag",
          "attrs": {
            "number": "34",
