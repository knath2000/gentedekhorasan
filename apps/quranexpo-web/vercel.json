{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": null,
  "functions": {
    "dist/server/entry.mjs": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/server/entry.mjs"
    }
  ],
  "env": {
    "PUBLIC_CLERK_PUBLISHABLE_KEY": "@public_clerk_publishable_key"
  }
}