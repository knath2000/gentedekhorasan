{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "paths": {
      "@clerk/clerk-react": ["./node_modules/@clerk/clerk-react"]
    },
    "types": ["react", "react-dom"]
  }
}